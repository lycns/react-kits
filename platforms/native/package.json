{
  "name": "native-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "upload": "smoex-scripts upload ./build",
    "build:pub": "npm run build:prod && npm run upload",
    "build:prod": "node ./bin/build android prod && node ./bin/build ios prod",
    "build": "node ./bin/build android && node ./bin/build ios",
